<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="skeleton.model">

    <class name="skeleton.model.Move" table="move">
        <id name="id" column="id_move">
            <generator class="increment"/>
        </id>
        <property name="username" type="string" column="username"/>
        <property name="currentState" type="string" column="current_state"/>
        <property name="punctaj" type="int" column="punctaj"/>
    </class>

</hibernate-mapping>
