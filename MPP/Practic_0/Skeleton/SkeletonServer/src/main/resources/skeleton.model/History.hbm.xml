<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="skeleton.model">

    <class name="History" table="history">
        <id name="id" column="id_history">
            <generator class="increment"/>
        </id>
        <property name="idGame" type="int" column="id_game"/>
        <property name="idRunda" type="int" column="id_runda"/>
        <property name="idPlayer" type="string" column="id_player"/>
        <property name="propunere" type="string" column="propunere"/>
        <property name="punctaj" type="int" column="punctaj"/>
    </class>

</hibernate-mapping>
